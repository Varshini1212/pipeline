pipeline{
  agent any
  stages{
    stage(checkout)
    {
      steps{
        echo "hello"
      }
    }
    stage(build)
    {
      steps{
        echo "world"
      }
    }
    stage(test)
    {
      steps{
        echo "varsha"
      }
    }
    stage(deploy)
    {
      steps{
        echo "colour blue"
      }
    }
  }
}

    
